{
  "name": "@polarsquad/cinode-api",
  "version": "0.4.0",
  "description": "Cinode API for the Cinode platform",
  "homepage": "https://github.com/polarsquad/cinode-api#readme",
  "bugs": {
    "url": "https://github.com/polarsquad/cinode-api/issues"
  },
  "author": "Tuomas Starck (tuomas.starck@polarsquad.com)",
  "contributors": [
    "Erno Aapa (erno@polarsquad.com)",
    "Tamas Kadar (tamas.kadar@polarsquad.com)",
    "Mikko Piuhola (mikko.piuhola@polarsquad.com)"
  ],
  "type": "module",
  "main": "./dist/index",
  "types": "./dist/index",
  "exports": {
    "./types": "./dist/types",
    "./api": "./dist/api",
    "./client": "./dist/client",
    "./config": "./dist/config",
    "./model": "./dist/model",
    "./service": "./dist/service",
    "./urls": "./dist/urls",
    "./utils": "./dist/utils"
  },
  "typesVersions": {
    "*": {
      "types": [
        "dist/types.d.ts"
      ],
      "api": [
        "dist/api.d.ts"
      ],
      "client": [
        "./dist/client.d.ts"
      ],
      "config": [
        "./dist/config.d.ts"
      ],
      "model": [
        "./dist/model.d.ts"
      ],
      "service": [
        "./dist/service.d.ts"
      ],
      "urls": [
        "./dist/urls.d.ts"
      ],
      "utils": [
        "./dist/utils.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "test": "mocha",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --check './src/**/*.ts'",
    "prettier-fix": "prettier --write './src/**/*.ts'",
    "validate": "tsc --noEmit",
    "full": "npm run validate && npm run lint && npm run prettier && npm run test",
    "prepare": "husky install",
    "prepublish": "rimraf dist && tsc --project tsconfig.build.json"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "bottleneck": "^2.19.5",
    "expiry-map": "^2.0.0",
    "google-spreadsheet": "^3.2.0",
    "got": "^12.0.3",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "p-memoize": "^6.0.1",
    "rimraf": "^3.0.2",
    "urlify": "^0.3.6"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "chai": "^4.3.6",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-jsdoc": "^38.1.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^7.0.4",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  }
}