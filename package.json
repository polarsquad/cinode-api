{
  "name": "@polarsquad/cinode-api",
  "version": "0.13.0",
  "description": "Cinode API for the Cinode platform",
  "homepage": "https://github.com/polarsquad/cinode-api#readme",
  "bugs": {
    "url": "https://github.com/polarsquad/cinode-api/issues"
  },
  "author": "Tuomas Starck (tuomas.starck@polarsquad.com)",
  "repository": {
    "url": "https://github.com/polarsquad/cinode-api"
  },
  "contributors": [
    "Erno Aapa (erno@polarsquad.com)",
    "Tamas Kadar (tamas.kadar@polarsquad.com)",
    "Mikko Piuhola (mikko.piuhola@polarsquad.com)"
  ],
  "type": "module",
  "typesVersions": {
    "*": {
      "*": [
        "dist/esm/*"
      ]
    }
  },
  "exports": {
    "./*": {
      "import": {
        "types": "./dist/esm/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "test": "mocha",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --check './**/*.ts'",
    "prettier-fix": "prettier --write './**/*.ts'",
    "validate": "tsc --noEmit",
    "full": "npm run validate && npm run lint && npm run prettier && npm run test",
    "prepare": "husky",
    "prepublishOnly": "rimraf dist && npm run build",
    "build": "tsc --project tsconfig.build.json"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "bottleneck": "^2.19.5",
    "expiry-map": "^2.0.0",
    "got": "^14.0.0",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "p-memoize": "^7.1.1",
    "urlify": "^0.3.6"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "chai": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-jsdoc": "^48.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^9.0.7",
    "mocha": "^10.2.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  }
}
